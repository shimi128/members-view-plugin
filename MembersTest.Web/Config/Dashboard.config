<?xml version="1.0" encoding="utf-8"?>
<dashBoard>
  <section alias="StartupDeveloperDashboardSection">
    <areas>
      <area>developer</area>
    </areas>
  </section>
  <section alias="StartupMediaDashboardSection">
    <areas>
      <area>media</area>
    </areas>
  </section>
  <section alias="StartupDashboardSection">
    <access>
      <deny>translator</deny>
    </access>
    <areas>
      <area>content</area>
    </areas>
    <tab caption="Get Started">
      <access>
        <grant>admin</grant>
      </access>
      <control showOnce="true" addPanel="true" panelCaption="">
        views/dashboard/default/startupdashboardintro.html
      </control>
    </tab>
    <tab caption="Change Password">
      <control showOnce="false" addPanel="false" panelCaption="">
          views/dashboard/ChangePassword.html
        </control>
    </tab>
    <tab caption="Members">
      <control addPanel="false" showOnce="true" panelCaption="">/app_plugins/Members/memberssashbord.html</control>
    </tab>
  </section>
  <section alias="StartupMemberDashboardSection">
    <areas>
      <area>member</area>
    </areas>
    <tab caption="Get Started">
      <control showOnce="true" addPanel="true" panelCaption="">
        views/dashboard/members/membersdashboardvideos.html
      </control>
    </tab>
  </section>
  <section alias="UmbracoBookshelfSection">
    <areas>
      <area>UmbracoBookshelf</area>
    </areas>
    <tab caption="Library">
      <control>/app_plugins/umbracobookshelf/backoffice/dashboards/library.html</control>
    </tab>
  </section>
</dashBoard>